<!-- container div -->
<div class="container">
    <br>
    <br>
    <center>
        <header id="showcase">
      <h1 class="cyan-text text-accent-3 font-style light italic">Leaderboard</h1>
    </header>
    <br>
        <table id="highScore" class="highlight-table">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Score</th>
                </tr>
            </thead>

            <tbody>

              {{#each Record}}
                <tr>
                    <td>{{this.dataValues.user_name}}</td>
                    <td>{{this.dataValues.score}}</td>
                </tr>
                {{/each}}

            </tbody>
        </table>
    </center>
    <!--end container div -->
</div>
